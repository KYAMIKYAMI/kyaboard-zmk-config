/ {
    key-matrix {
        compatible = "zmk,matrix-transform";
        diode-direction = "col2row";
        row-gpios = <&gpio0 2 0>, <&gpio0 3 0>, <&gpio0 28 0>, <&gpio0 29 0>;
        col-gpios = <&gpio0 4 0>, <&gpio0 5 0>, <&gpio1 11 0>,
                    <&gpio1 12 0>, <&gpio1 13 0>, <&gpio1 14 0>,
                    <&gpio1 15 0>, <&gpio1 3 0>, <&gpio1 10 0>;
    };
};
